<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
                                   "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping default-lazy="false">
 <class dynamic-insert="true" dynamic-update="true" lazy="false"
  name="org.ana.wcmp.database.dao.UserRoleDAO" table="T_USER_ROLE">
  <id name="pk_user_role" type="long">
   <generator class="identity"/>
  </id>
  <property generated="never" lazy="false" length="45"
   name="user_role_name" not-null="true" type="string"/>
  <property generated="never" lazy="false" name="user_role_desc" type="blob"/>
 </class>
</hibernate-mapping>
