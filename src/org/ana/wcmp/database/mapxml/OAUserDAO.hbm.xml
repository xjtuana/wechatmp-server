<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
                                   "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping default-lazy="false">
 <class dynamic-insert="true" dynamic-update="true" lazy="false"
  name="org.ana.wcmp.database.dao.OAUserDAO" table="T_OA_USER">
  <id name="pk_oa_user" type="long">
   <generator class="identity"/>
  </id>
  <property length="45" name="oa_user_name" not-null="true"
   type="string" unique="true"/>
  <property length="128" name="oa_user_password" not-null="true" type="string"/>
 </class>
</hibernate-mapping>
