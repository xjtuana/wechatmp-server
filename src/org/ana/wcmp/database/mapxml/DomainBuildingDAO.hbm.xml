<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
                                   "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping default-lazy="false">
	<class dynamic-insert="true" dynamic-update="true" lazy="false"
		name="org.ana.wcmp.database.dao.DomainBuildingDAO" table="T_DOMAIN_BUILDING">
		<id name="pk_domain_building" type="long">
			<generator class="identity" />
		</id>
		<property generated="never" lazy="false" length="45"
			name="domain_building_name" not-null="true" type="string" />
		<property generated="never" lazy="false" name="domain_building_desc"
			type="blob" />
		<many-to-one cascade="save-update" class="org.ana.wcmp.database.dao.GroupDAO"
			column="domain_building_group_id" fetch="join" lazy="false"
			name="domain_building_group" not-null="false" />
		<many-to-one cascade="save-update"
			class="org.ana.wcmp.database.dao.DomainAreaDAO" column="domain_building_area_id"
			fetch="join" name="domain_building_area" not-null="false" />
	</class>
</hibernate-mapping>
